    "parallel-mocha": "*",
    "prop-types": "15.6.2",
    "react": "16.6.1",
    "react-dom": "16.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "redux": "3.7.2",
    "redux-thunk": "2.3.0",
